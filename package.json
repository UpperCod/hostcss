{
	"name": "@atomico/hostcss",
	"description": "css-in-js, small and minimalist",
	"version": "0.2.2",
	"main": "dist/atomico-hostcss.js",
	"module": "dist/atomico-hostcss.mjs",
	"unpkg": "dist/atomico-hostcss.umd.js",
	"source": "src/index.js",
	"author": "Matias Trujillo Olivares",
	"license": "ISC",
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/atomicojs/hostcss.git"
	},
	"scripts": {
		"test": "jest",
		"build": "rollup -c",
		"watch": "rollup -c -w",
		"prepare": "npm run build"
	},
	"peerDependencies": {
		"@atomico/core": ">=1.3.10"
	},
	"devDependencies": {
		"@babel/core": "^7.2.2",
		"@babel/preset-env": "^7.3.1",
		"@babel/preset-react": "^7.0.0",
		"babel-core": "^6.26.3",
		"babel-jest": "^24.1.0",
		"babel-preset-env": "^1.7.0",
		"jest": "^24.1.0",
		"regenerator-runtime": "^0.13.1",
		"rollup": "^1.1.2",
		"rollup-plugin-buble": "^0.19.6",
		"rollup-plugin-size-snapshot": "^0.8.0",
		"rollup-plugin-terser": "^4.0.3"
	},
	"babel": {
		"presets": [
			"@babel/preset-env",
			[
				"@babel/preset-react",
				{
					"pragma": "h",
					"throwIfNamespace": false
				}
			]
		]
	},
	"jest": {
		"testRegex": "./test/.*.test.js$",
		"rootDir": "."
	}
}
